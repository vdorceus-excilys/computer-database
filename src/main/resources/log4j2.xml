<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="ALL" name="cdb" packages="">
	<Appenders>
        <Console name="stdout" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %p %m%n"/>
        </Console>

	<File name="fout" fileName="cdb.log" append="true">
        <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %p %m%n"/>
    	</File>

    </Appenders>

<Loggers>

<Root level="ALL">
	<AppenderRef ref="stdout"/>
	<AppenderRef ref="fout"/>
</Root>	
</Loggers>
</Configuration>
